<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="100.0" minWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox alignment="CENTER_LEFT" spacing="12.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Sound sources">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#createSource" text="+" />
         </children>
         <padding>
            <Insets bottom="8.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" minHeight="80.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="sources" />
         </content>
      </ScrollPane>
   </center>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</BorderPane>
